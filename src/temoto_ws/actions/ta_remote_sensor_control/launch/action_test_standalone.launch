<launch>
  <arg name="umrf_file" default="$(find ta_remote_sensor_control)/test/invoker_umrf.json"/>
  <arg name="wake_word" default="ta_remote_sensor_control"/>
  <arg name="action_path" default="$(find ta_remote_sensor_control)"/>

  <node name="temoto_action_engine" pkg="temoto_action_engine" type="action_engine_node" output="screen"
        args="--sa $(arg action_path) 
              --d $(arg umrf_file) 
              --mw $(arg wake_word)"/>
</launch>