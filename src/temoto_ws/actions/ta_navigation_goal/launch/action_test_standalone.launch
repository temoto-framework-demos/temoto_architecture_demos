<launch>
  <arg name="umrf_file" default="$(find ta_navigation_goal)/test/ta_navigation_goal.umrfg.json"/>
  <arg name="wake_word" default="ta_navigation_goal"/>
  <arg name="action_path" default="$(find ta_navigation_goal)"/>

  <node name="temoto_action_engine" pkg="temoto_action_engine" type="action_engine_node" output="screen"
        args="--sa $(arg action_path) 
              --d $(arg umrf_file)
              --mw $(arg wake_word)"/>
</launch>