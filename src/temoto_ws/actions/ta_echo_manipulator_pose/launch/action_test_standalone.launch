<launch>
  <arg name="umrf_file" default="$(find ta_echo_manipulator_pose)/test/ta_echo_manipulator_pose.umrfg.json"/>
  <arg name="wake_word" default="ta_echo_manipulator_pose"/>
  <arg name="action_path" default="$(find ta_echo_manipulator_pose)"/>

  <node name="temoto_action_engine" pkg="temoto_action_engine" type="action_engine_node" output="screen"
        args="--sa $(arg action_path) 
              --d $(arg umrf_file)
              --mw $(arg wake_word)"/>
</launch>